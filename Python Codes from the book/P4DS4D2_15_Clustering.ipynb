    "# Agrupando com K-Means\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "A = np.array([165, 55, 70])\n",
    "B = np.array([185, 60, 30])\n",
    "\n",
    "D = (A - B)\n",
    "D = D**2\n",
    "D = np.sqrt(np.sum(D))\n",
    "print(D)"
   ]
  }
